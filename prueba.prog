principal {
  
  ent n, curr;
    

    salida("introduce");
    entrada(n);
    salida(" ", n, " == ");
    curr = 2;

    mientras ( curr <= n ) {

      ent d;
      d = n / curr;
      si d * curr == n {
        salida("* ", curr, " ");
        n = n / curr;
      }sino{
        curr = curr+1;
      } 
    
    }
    salida("\n");
  devolver 0;
}
